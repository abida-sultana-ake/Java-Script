const numbers = [1, 2, 3, 4, 5, 6, 7];
//mutable --> changeable
// immutable --> unchangeable

// method - 1
console.log(numbers);
numbers.reverse();
console.log(numbers);

// method - 2
const rev_numbers = [];
for(num of numbers)
{
    console.log(num);
    rev_numbers.unshift;
}
console.log(rev_numbers);
