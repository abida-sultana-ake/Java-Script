const person = {
    name : "Azan",  // key : Value
    age : 10,
    country : "Bangladesh"
}
Object.seal(person);

person.status = "Not found";
console.log(person);

console.log(Object.keys(person)); // ['name', 'age', 'country']
console.log(Object.values(person)); // ['Azan', 10, 'Bangladesh']
console.log(Object.entries(person)); //  Â [Array(2), Array(2), Array(2)]
