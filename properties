/*
Count the number of properties.

Input:

let student = {
    name: 'Ariana Grande',
    age: 21,
    city: 'Gaibandha',
    isStudent: true
};
Output:


4
*/


let student = {
    name : 'Abida Sultana',
    age : 21,
    city : 'Dhaka',
    isStudent : true
}
let propertyCount = Object.keys(student).length;
console.log(propertyCount);
